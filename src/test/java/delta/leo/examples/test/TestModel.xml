<?xml version="1.0"?>
<!--<!DOCTYPE MODEL SYSTEM 'delta/leo/model/Model.dtd'> -->
<MODEL>
	<ENTITY NAME="B" SUPER="A">
		<FIELD NAME="INDEX">
			<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
			<TYPE CLASS="INTEGER">
				<PARAM NAME="MIN" VALUE="1" />
				<PARAM NAME="MAX" VALUE="5" />
			</TYPE>
		</FIELD>
		<FIELD NAME="NAME">
			<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
			<TYPE CLASS="STRING">
				<PARAM NAME="MIN_LENGTH" VALUE="3" />
				<PARAM NAME="MAX_LENGTH" VALUE="10" />
				<PARAM NAME="MULTILINES" VALUE="true" />
			</TYPE>
		</FIELD>
		<FIELD NAME="OPTIONS">
			<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
			<TYPE CLASS="CHOICE">
				<PARAM NAME="MULTIPLE" VALUE="true" />
				<PARAM NAME="OPTION" VALUE="OPTION1|1" />
				<PARAM NAME="OPTION" VALUE="OPTION2|2" />
				<PARAM NAME="OPTION" VALUE="OPTION3|3" />
			</TYPE>
		</FIELD>
		<FIELD NAME="BIRTHDATE">
			<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
			<TYPE CLASS="DATE" />
		</FIELD>
	</ENTITY>
	<ENTITY NAME="A">
		<FIELD NAME="NAME">
			<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
			<TYPE CLASS="STRING">
				<PARAM NAME="MIN_LENGTH" VALUE="3" />
				<PARAM NAME="MAX_LENGTH" VALUE="10" />
				<PARAM NAME="MULTILINES" VALUE="true" />
			</TYPE>
		</FIELD>
		<COMPOUND_FIELD NAME="MY_COMPOUND">
			<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
			<FIELDS>
				<FIELD NAME="INDEX">
					<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
					<TYPE CLASS="INTEGER">
						<PARAM NAME="MIN" VALUE="1" />
						<PARAM NAME="MAX" VALUE="5" />
					</TYPE>
				</FIELD>
				<FIELD NAME="NAME">
					<PROPERTIES MANDATORY="true" ID="true" NAME="false"/>
					<TYPE CLASS="STRING">
						<PARAM NAME="MIN_LENGTH" VALUE="3" />
						<PARAM NAME="MAX_LENGTH" VALUE="10" />
						<PARAM NAME="MULTILINES" VALUE="false" />
					</TYPE>
				</FIELD>
			</FIELDS>
		</COMPOUND_FIELD>
	</ENTITY>
	<BINARY_ASSOCIATION NAME="A AND B">
		<A ENTITY_NAME="A" ROLE="A_ROLE"/>
		<B ENTITY_NAME="B" ROLE="B_ROLE"/>
		<AtoB MIN="0" MAX="INFINITY" ORDERED="true" TRAVERSABLE="true" MEANING="A TO B"/>
		<BtoA MIN="1" MAX="3" ORDERED="false" TRAVERSABLE="false" MEANING="B TO A"/>
	</BINARY_ASSOCIATION>
	<COMPOSITION NAME="A INCLUDES B" SOURCE="A" MIN="1" MAX="5" TARGET="B" />
</MODEL>
